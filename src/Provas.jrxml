	<queryString>
		<![CDATA[SELECT
	Questoes.Questoes_ID,
	Questoes.Enunciado,
	Questoes.Dificuldade,
	Questoes.ID_Conteudos,
	Questoes.MultiplaEscolha,
	CASE WHEN Questoes.AlternativaA <> '' THEN CONCAT('A) ', Questoes.AlternativaA) END AS AlternativaA,
	CASE WHEN Questoes.AlternativaB <> '' THEN CONCAT('B) ', Questoes.AlternativaB) END AS AlternativaB,
	CASE WHEN Questoes.AlternativaC <> '' THEN CONCAT('C) ', Questoes.AlternativaC) END AS AlternativaC,
	CASE WHEN Questoes.AlternativaD <> '' THEN CONCAT('D) ', Questoes.AlternativaD) END AS AlternativaD,
	CASE WHEN Questoes.AlternativaE <> '' THEN CONCAT('E) ', Questoes.AlternativaE) END AS AlternativaE,
	CASE WHEN Questoes.AlternativaF <> '' THEN CONCAT('F) ', Questoes.AlternativaF) END AS AlternativaF,
	ImagemQuest.ImagemQuest_ID,
	ImagemQuest.ID_Questao,
	ImagemQuest.Imagem,
	ImagemQuest.POSICAO,
	ImagemQuest.NomeImagem
FROM
	Questoes INNER JOIN ImagemQuest ON Questoes.Questoes_ID = ImagemQuest.ID_Questao]]>
